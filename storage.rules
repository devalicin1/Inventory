rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Public access for logo files only
    match /Logos/{fileName} {
      allow read: if true;
      allow write: if false; // Only allow manual uploads via Firebase Console
    }
    
    // All other paths require authentication
    match /{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}

